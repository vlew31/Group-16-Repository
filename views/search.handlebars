
<section>
    <header>
        <h1>Listtings Found</h1>
        <h2>Search results for: {{ searchTerm }}</h2>
    </header>
    <ol class="clothes-list">
        {{#each clothes}}
        <li>
            {{ clothes }}
        </li>
        {{/each}}
    </ol>
    {{else}}
    <p>No listings found for "{{ searchTerm }}"</p>
    {{/if}}
    <a href="/">Search for Another Item</a>
</section>